<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cflink="http://eurocris.org/cerif/annotations#" xmlns:cfprocess="http://eurocris.org/cerif/preprocessing#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3.org/2001/XMLSchema https://www.w3.org/2012/04/XMLSchema.xsd http://eurocris.org/cerif/annotations# http://eurocris.org/cerif/xml/schemas/annotations.xsd"
	elementFormDefault="qualified" attributeFormDefault="unqualified" xml:lang="en">

	<xs:annotation>
		<xs:documentation>This is the XML Schema for CERIF (Common European Research Information Format) XXX release, developed by the euroCRIS CERIF Task Group.
			This work is licensed under a Creative Commons Attribution 4.0 International License (http://creativecommons.org/licenses/by/4.0/).
			For more information about CERIF and euroCRIS see http://www.eurocris.org/.
		</xs:documentation>
	</xs:annotation>

	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd">
		<xs:annotation>
			<xs:documentation>The <code>xml:lang</code> attribute.</xs:documentation>
		</xs:annotation>
	</xs:import>
	
	<xs:element name="Individual__SubstitutionGroupHead" type="cfIdAttr__BaseType" abstract="true">
		<xs:annotation>
			<xs:documentation>Any CERIF base object</xs:documentation>
		</xs:annotation>
	</xs:element>
	
	<xs:element name="PersonOrOrgUnit__SubstitutionGroupHead" substitutionGroup="Individual__SubstitutionGroupHead" type="PersonOrOrgUnit__BaseType" abstract="true">
		<xs:annotation>
			<xs:documentation>A person or an organisational unit</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PersonOrOrgUnit__BaseType">
		<xs:annotation>
			<xs:documentation>A common supertype for persons and organisational units</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cfIdAttr__BaseType"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="Person" substitutionGroup="PersonOrOrgUnit__SubstitutionGroupHead" cflink:entity="cfPers">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="PersonOrOrgUnit__BaseType">
					<xs:sequence>

						<xs:element name="PersonName" type="cfPersName__Type" minOccurs="0" cflink:entity="cfPersName">
							<xs:annotation>
								<xs:documentation>The name of the person</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="Type" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfPers_Class" cfprocess:expandBy="Classification(Type)">
							<xs:annotation>
								<xs:documentation>The type of the person</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="Identifier" type="cfGenericIdentifier__Type" minOccurs="0" maxOccurs="unbounded" cflink:identifier="true" cfprocess:expandBy="http://eurocris.org/cerif/vocab/IdentifierTypes#" />

						<xs:element name="ElectronicAddress" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded" cflink:entity="cfEAddr" />

						<!-- ... -->

						<xs:group ref="__TheRestGroup"/>
						
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="cfPersName__Type">
		<xs:complexContent>
			<xs:extension base="cfIdAttr__BaseType">
				<xs:sequence>

					<xs:element name="FamilyNames" type="xs:string" minOccurs="0" cflink:attribute="cfPersName.cfFamilyNames" />

					<xs:element name="FirstNames" type="xs:string" minOccurs="0" cflink:attribute="cfPersName.cfFirstNames" />

					<xs:element name="OtherNames" type="xs:string" minOccurs="0" cflink:attribute="cfPersName.cfOtherNames" />

					<xs:element name="Type" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfPersName_Class" cfprocess:expandBy="Classification(Type)" />

					<xs:group ref="__TheRestGroup"/>
						
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="OrgUnit" substitutionGroup="PersonOrOrgUnit__SubstitutionGroupHead" cflink:entity="cfOrgUnit">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="PersonOrOrgUnit__BaseType">
					<xs:sequence>

						<xs:element name="Acronym" type="cfString__Type" minOccurs="0" cflink:attribute="cfOrgUnit.cfAcro">
							<xs:annotation>
								<xs:documentation>The acronym of the organisational unit</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="Name" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="cfOrgUnitName.cfName">
							<xs:annotation>
								<xs:documentation>The name of the organisational unit</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="Identifier" type="cfGenericIdentifier__Type" minOccurs="0" maxOccurs="unbounded" cflink:identifier="true" cfprocess:expandBy="http://eurocris.org/cerif/vocab/IdentifierTypes#">
							<xs:annotation>
								<xs:documentation>An identifier of the organisational unit</xs:documentation>
							</xs:annotation>
						</xs:element>

						<!-- ... -->

						<xs:element name="ElectronicAddress" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded" cflink:entity="cfEAddr">
							<xs:annotation>
								<xs:documentation>An electronic address associated with the organisational unit</xs:documentation>
							</xs:annotation>
						</xs:element>

						<!-- ... -->

						<xs:element name="PartOf" minOccurs="0" cflink:link="cfOrgUnit_OrgUnit(http://eurocris.org/cerif/vocab/Inter-­OrganisationRelations#Part):1">
							<xs:annotation>
								<xs:documentation>Chain up to the larger unit that encompasses this unit</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="cfLinkWithDisplayName__BaseType">
										<xs:sequence>
											<xs:element ref="OrgUnit" />
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>

						<xs:element name="Parts" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The list of parts (smaller units) of this unit (container element)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Part" minOccurs="0" maxOccurs="unbounded" cflink:link="cfOrgUnit_OrgUnit(http://eurocris.org/cerif/vocab/Inter-­OrganisationRelations#Part):2">
										<xs:annotation>
											<xs:documentation>The relationship to the contained unit</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="cfLinkWithDisplayName__BaseType">
													<xs:sequence>
														<xs:element ref="OrgUnit" />
													</xs:sequence>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<!-- ... -->

						<xs:group ref="__TheRestGroup"/>
						
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="ResearchOutput__SubstitutionGroupHead" substitutionGroup="Individual__SubstitutionGroupHead" type="ResearchOutput__BaseType" abstract="true">
		<xs:annotation>
			<xs:documentation>Research outputs: publications, patents, and products</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ResearchOutput__BaseType">
		<xs:annotation>
			<xs:documentation>A common supertype for research outputs: publications, patents, and products</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cfIdAttr__BaseType"/>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="Publication" substitutionGroup="ResearchOutput__SubstitutionGroupHead" cflink:entity="cfResPubl">
		<xs:annotation>
			<xs:documentation source="http://dictionary.casrai.org/research-personnel-profile/1.1.0/contributions/outputs/publications">Research or scholarly published output resulting from, or related to, research activities.</xs:documentation>
		</xs:annotation>
		<!-- link to the CERIF data model entity -->
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="ResearchOutput__BaseType">
					<xs:sequence>

						<xs:element cflink:attribute="cfResPublTitle.cfTitle" name="Title" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>The title of the publication</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:attribute="cfResPublSubtitle.cfSubtitle" name="Subtitle" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>The subtitle of the publication</xs:documentation>
							</xs:annotation>
						</xs:element>

						<!-- any class scheme of the 'type' character -->
						<xs:element cflink:link="cfResPubl_Class" name="Type" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cfprocess:expandBy="Classification(Type)">
							<xs:annotation>
								<xs:documentation>The type of the publication</xs:documentation>
							</xs:annotation>
						</xs:element>

						<!-- a binary link to the Publishing Channel entity -->
						<xs:element cflink:link="cfResPubl_PublChannel(http://eurocris.org/cerif/vocab/PublicationChannelLinks#PublishedIn)" name="PublishedIn" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Link to the Publishing Channel where this publication appeared</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="cfLink__BaseType">
										<xs:sequence>
											<xs:element ref="PublishingChannel" cfprocess:expandBy="http://eurocris.org/cerif/vocab/PublishingChannelTypes#" />
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>

						<xs:element cflink:link="cfResPubl_ResPubl(http://eurocris.org/cerif/vocab/Inter-­PublicationRelations#Part):1" name="PartOf" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Link to the Publication of which this publication is a part (e.g. a book chapter links to the book that contains it)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="cfLinkWithDisplayName__BaseType">
										<xs:sequence>
											<xs:element ref="Publication" />
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfResPublDate" name="PublicationDate" type="cfDate__Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The date the publication appeared</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfNum" name="Number" type="cfString__Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The number of the publication (e.g. Article Number)</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfVol" name="Volume" type="cfString__Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The volume of the publishing channel where this publication appeared</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfIssue" name="Issue" type="cfString__Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The issue of the publishing channel where this publication appeared</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfEdition" name="Edition" type="cfString__Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The edition of the publication</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfSeries" name="Series" type="cfString__Type" minOccurs="0">
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfStartPage" name="StartPage" type="cfString__Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The page where this publication starts, in case the publishing channel or containing publication has numbered pages</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfEndPage" name="EndPage" type="cfString__Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The page where this publication ends, in case the publishing channel or containing publication has numbered pages</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:attribute="cfResPubl.cfTotalPages" name="TotalPages" type="cfNonnegativeInteger__Type" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The number of pages of this publication</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:identifier="true" name="Identifier" type="cfGenericIdentifier__Type" minOccurs="0" maxOccurs="unbounded" cfprocess:expandBy="http://eurocris.org/cerif/vocab/IdentifierTypes#" />

						<xs:element cflink:container="ordered" name="Authors" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The authors of this publication</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Author" type="cfLinkWithDisplayNameToPersonWithAffiliationsOrOrgUnit__Type" minOccurs="0" maxOccurs="unbounded" 
										cflink:link="cfResPubl_Pers(http://eurocris.org/cerif/vocab/PersonOutputContributions#Author) cfResPubl_OrgUnit(http://eurocris.org/cerif/vocab/OrganisationOutputContributions#Author)" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element cflink:container="ordered" name="Editors" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The authors of this publication</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Editor" type="cfLinkWithDisplayNameToPersonWithAffiliationsOrOrgUnit__Type" minOccurs="0" maxOccurs="unbounded"
										cflink:link="cfResPubl_Pers(http://eurocris.org/cerif/vocab/PersonOutputContributions#Editor) cfResPubl_OrgUnit(http://eurocris.org/cerif/vocab/OrganisationOutputContributions#Editor)" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element cflink:container="ordered" name="Publishers" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The publishers of this publication</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Publisher" type="cfLinkWithDisplayNameToPersonOrOrgUnit__Type" minOccurs="0" maxOccurs="unbounded"
										cflink:link="cfResPubl_Pers(http://eurocris.org/cerif/vocab/PersonOutputContributions#Publisher) cfResPubl_OrgUnit(http://eurocris.org/cerif/vocab/OrganisationOutputContributions#Publisher)" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="License" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfResPubl_Class" cfprocess:expandBy="Classification(License)" /><!-- any 
							class scheme of the 'license' character -->

						<xs:element name="Subject" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfResPubl_Class" cfprocess:expandBy="Classification(Subject)" /><!-- any 
							class scheme of the 'subject' character -->

						<xs:element name="Keyword" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="cfResPublKeyw.cfKeyw" />

						<xs:element name="Abstract" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="cfResPublAbstr.cfAbstr" />

						<xs:element name="Status" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfResPubl_Class" cfprocess:expandBy="Classification(Status)" /><!-- any 
							class scheme of the 'subject' character -->

						<xs:element name="Originator" minOccurs="0" maxOccurs="unbounded" cflink:link="cfProj_ResPubl(http://eurocris.org/cerif/vocab/Project_Output_Roles#Originator)">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="cfLink__BaseType">
										<xs:sequence>
											<xs:element ref="Project"/>
										</xs:sequence>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						
						<xs:group ref="__TheRestGroup"/>
						
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="PublishingChannel" cflink:entity="Publishing Channel">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="cfIdAttr__BaseType">
					<xs:sequence>

						<xs:element name="Title" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="cfPublChanTitle.cfTitle" />

						<xs:element name="Type" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfPublChan_Class" cfprocess:expandBy="Classification(Type)" />

						<xs:element name="Identifier" type="cfGenericIdentifier__Type" minOccurs="0" maxOccurs="unbounded" cflink:identifier="true" cfprocess:expandBy="http://eurocris.org/cerif/vocab/IdentifierTypes#" />

						<xs:element name="Publishers" minOccurs="0" cflink:orderedContainer="true">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Publisher" type="cfLinkWithDisplayNameToPersonOrOrgUnit__Type" minOccurs="0" maxOccurs="unbounded"
										cflink:link="cfPublChan_Pers(http://eurocris.org/cerif/vocab/PersonOutputContributions#Publisher) cfPublChan_OrgUnit(http://eurocris.org/cerif/vocab/OrganisationOutputContributions#Publisher)" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:group ref="__TheRestGroup"/>
						
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="Project" substitutionGroup="Individual__SubstitutionGroupHead" cflink:entity="cfProj">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="cfIdAttr__BaseType">
					<xs:sequence>
						
						<xs:element name="Type" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfProj_Class" cfprocess:expandBy="Classification(Type)">
							<xs:annotation>
								<xs:documentation>The type of the project</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="Acronym" type="cfString__Type" minOccurs="0" cflink:attribute="cfProj.cfAcro">
							<xs:annotation>
								<xs:documentation>The acronym of the project</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="Title" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="cfProjTitle.cfTitle">
							<xs:annotation>
								<xs:documentation>The title of the project</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="Identifier" type="cfGenericIdentifier__Type" minOccurs="0" maxOccurs="unbounded" cflink:identifier="true" cfprocess:expandBy="http://eurocris.org/cerif/vocab/IdentifierTypes#">
							<xs:annotation>
								<xs:documentation>An identifier of the project</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="StartDate" type="cfDate__Type" minOccurs="0" cflink:attribute="cfProj.cfStartDate">
							<xs:annotation>
								<xs:documentation>The start date of the project</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="EndDate" type="cfDate__Type" minOccurs="0" cflink:attribute="cfProj.cfEndDate">
							<xs:annotation>
								<xs:documentation>The end date of the project</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="Funders" minOccurs="0" cflink:unorderedContainer="true">
							<xs:annotation>
								<xs:documentation>The funders who provide funding to the project</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Funder" type="cfLinkWithDisplayNameToPersonOrOrgUnit__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="">
										<xs:annotation>
											<xs:documentation>The funder of the project</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						
						<xs:element name="Consortium" minOccurs="0" cflink:unorderedContainer="true">
							<xs:annotation>
								<xs:documentation>The consortium of the project: the organisations (persons) who are contractually bound to do the work in the project</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:sequence cfprocess:extensionPoint="AdditionalProjectConsortiumRoles"/>
									<xs:element name="Member" type="cfLinkWithDisplayNameToPersonOrOrgUnit__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfProj_OrgUnit cfProj_Pers">
										<xs:annotation>
											<xs:documentation>A member of the project consortium</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						
						<xs:element name="Team" minOccurs="0" cflink:unorderedContainer="true">
							<xs:annotation>
								<xs:documentation>The project team: the persons who carry out the work in the project, typically as a part of their job at the organisations from the consortium</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="PrincipalInvestigator" type="cfLinkWithDisplayNameToPersonWithAffiliations__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfProj_Pers">
										<xs:annotation>
											<xs:documentation>The principal investigator: the person responsible for the whole project, the head of the project team</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Contact" type="cfLinkWithDisplayNameToPersonWithAffiliations__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfProj_Pers">
										<xs:annotation>
											<xs:documentation>A person to contact in matters connected with her/his organisations' participation in the project</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:sequence cfprocess:extensionPoint="AdditionalProjectTeamRoles"/>
									<xs:element name="Member" type="cfLinkWithDisplayNameToPersonWithAffiliations__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfProj_Pers">
										<xs:annotation>
											<xs:documentation>A member of the project team</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						
						<xs:sequence cfprocess:extensionPoint="ProjectSubjectClassifications"/>
						<xs:element name="Subject" type="cfGenericClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="cfProj_Class" cfprocess:expandBy="Classification(Subject)">
							<xs:annotation>
								<xs:documentation>The subject classification(s) of the project</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="Keyword" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="cfProjKeyw.cfKeyw">
							<xs:annotation>
								<xs:documentation>The keywords of the project</xs:documentation>
							</xs:annotation>
						</xs:element>

