		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="cfIdAttr__BaseType">
					<xs:sequence>
						
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
